@import "materialize";

@media only screen and (min-width: 601px){

  	nav a.brand-logo{
  		padding-top: 4px;
  		padding-left: 40px;
  		top: -2px;
  		left: 0;
  	}
}

nav span#logo-text, nav ul a{
	color: #646464;
}
nav span#logo-text{
	margin-left: 10px;
 	vertical-align: top;
}

nav .contenedor{
	display: flex;
  	justify-content: center;
}

nav ul#nav-sesion{
	position: absolute;
  	right: 0px;
}
nav ul#nav-sesion li{
	position: relative;
}
nav ul#nav-sesion li:hover{
	background: transparent;
}
nav ul#nav-sesion li:first-child:after{
	content: "|";
	display: inline-block;
	color: #646464;
	position: absolute;
	top: 0;
	right: 0;
}



nav ul#nav-sesion li a{
	display: inline;
}

